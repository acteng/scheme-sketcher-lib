{
  "name": "scheme-sketcher-lib",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepare": "npm run package",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check .",
    "fmt": "prettier --write ."
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/geojson": "^7946.0.14",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "publint": "^0.1.9",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.11"
  },
  "dependencies": {
    "@maptiler/geocoding-control": "^1.3.1",
    "@turf/mask": "^7.0.0",
    "@turf/nearest-point-on-line": "^7.0.0",
    "govuk-svelte": "github:acteng/govuk-svelte",
    "maplibre-draw-polygon": "github:dabreegster/maplibre-draw-polygon",
    "maplibre-gl": "^4.0.2",
    "route-snapper": "^0.3.0",
    "route-snapper-ts": "^0.0.5",
    "svelte-maplibre": "^0.9.7"
  },
  "overrides": {
    "route-snapper-ts": {
      "route-snapper": "^0.3.0"
    }
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
