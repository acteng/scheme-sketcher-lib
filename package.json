{
  "name": "scheme-sketcher-lib",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepare": "npm run package",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check .",
    "fmt": "prettier --write .",
    "setup-govuk": "sass src/style/main.sass src/style/main.css; rm -rf static/assets; mkdir -p static/assets; cp -R node_modules/govuk-frontend/dist/govuk/assets/ static/"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./common": {
      "types": "./dist/common/index.d.ts",
      "svelte": "./dist/common/index.js",
      "import": "./dist/common/index.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "svelte": "./dist/config.js",
      "import": "./dist/config.js"
    },
    "./draw": {
      "types": "./dist/draw/index.d.ts",
      "svelte": "./dist/draw/index.js",
      "import": "./dist/draw/index.js"
    },
    "./draw/route": {
      "types": "./dist/draw/route/index.d.ts",
      "svelte": "./dist/draw/route/index.js",
      "import": "./dist/draw/route/index.js"
    },
    "./draw/stores": {
      "types": "./dist/draw/stores.d.ts",
      "svelte": "./dist/draw/stores.js",
      "import": "./dist/draw/stores.js"
    },
    "./draw/types": {
      "types": "./dist/draw/types.d.ts",
      "svelte": "./dist/draw/types.js",
      "import": "./dist/draw/types.js"
    },
    "./maplibre": {
      "types": "./dist/maplibre/index.d.ts",
      "svelte": "./dist/maplibre/index.js",
      "import": "./dist/maplibre/index.js"
    },
    "./sidebar": {
      "types": "./dist/sidebar/index.d.ts",
      "svelte": "./dist/sidebar/index.js",
      "import": "./dist/sidebar/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.2",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/geojson": "^7946.0.14",
    "@types/uuid": "^10.0.0",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "publint": "^0.1.9",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.11",
    "vite-plugin-wasm-pack": "0.1.11"
  },
  "dependencies": {
    "@maptiler/geocoding-control": "1.2.3",
    "@turf/mask": "^7.0.0",
    "@turf/nearest-point-on-line": "^7.0.0",
    "govuk-frontend": "^5.4.1",
    "govuk-svelte": "github:acteng/govuk-svelte",
    "maplibre-gl": "^4.0.2",
    "route-snapper": "^0.4.7",
    "route-snapper-ts": "^0.0.8",
    "sass": "^1.77.8",
    "svelte-maplibre": "^0.9.14",
    "uuid": "^10.0.0"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
